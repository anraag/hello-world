// to reverse the elements in a array
#include <iostream>
#include <string>
#include <array>
using namespace std;

int main()
{
  int anurag[5]{1,2,3,4,5};
  int name[5];
  int i=0;
  for(i=0;i<5;i++){
      int temp=anurag[4-i];
      name[i]=temp;
  }
  for (i=0;i<5;i++){
      cout<<name[i];
  }
  return 0;
}
